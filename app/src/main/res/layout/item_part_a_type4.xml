<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <TextView
        android:id="@+id/tv_item_part_a_type4_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="1a. Type of Building/Land Use"
        android:textSize="12sp"
        android:textColor="@color/colorBlack"
        android:layout_marginLeft="@dimen/spot_weight"
        android:layout_marginTop="20dp"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp">
        <EditText
            android:id="@+id/et_item_part_a_type4_1"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="@drawable/bg_item_type2"
            android:layout_marginLeft="@dimen/spot_weight"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:textSize="12sp"/>

        <EditText
            android:id="@+id/et_item_part_a_type4_2"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="@drawable/bg_item_type2"
            android:layout_marginLeft="40dp"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:textSize="12sp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="Service"
            android:textSize="11sp"
            android:gravity="center_vertical"
            android:layout_marginLeft="10dp"
            />

        <EditText
            android:id="@+id/et_item_part_a_type4_3"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="@drawable/bg_item_type2"
            android:layout_marginLeft="@dimen/spot_weight"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:textSize="12sp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="Vehicle"
            android:textSize="11sp"
            android:gravity="center_vertical"
            android:layout_marginLeft="10dp"
            />
    </LinearLayout>

</LinearLayout>
